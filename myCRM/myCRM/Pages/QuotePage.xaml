<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="myCRM.Pages.QuotePage">
	<ContentPage.Content>
		<StackLayout>
			<Label >Customer Name</Label><Entry Text="{Binding Quote.CustomerName}"></Entry>
			<Label>Quote Amount</Label><Entry Text="{Binding Quote.QuoteAmount}"></Entry>
		</StackLayout>
	</ContentPage.Content>
</ContentPage>